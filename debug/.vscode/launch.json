    "version": "0.2.0",
    "configurations": [
        {
            "name": "Remote on Localhost",
            "type": "go",
            "request": "launch",
            "mode": "remote",
            "remotePath": "${workspaceRoot}",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${workspaceRoot}",
            "env": {},
            "showLog": true,
            "trace": "verbose"
        },
        {
            "name": "Remote on Docker",
            "type": "go",
            "request": "launch",
            "mode": "remote",
            "remotePath": "/go/src/github.com/mateuszdyminski/go-diagnose",
            "port": 40000,
            "host": "127.0.0.1",
            "program": "${workspaceRoot}",
            "env": {},
            "showLog": true,
            "trace": "verbose"
        },
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}",
            "env": {},
            "args": [],
            "showLog": true,
            "trace": "verbose"
        }
    ]
}